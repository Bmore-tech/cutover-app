<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmodelo.cutoverback.Activity">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!-- Compliant -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- Compliant -->
    <uses-permission android:name="android.permission.CAMERA" /> <!-- Compliant -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- Compliant -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /> <!-- Compliant -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_i360_launcher_beta_01_round"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_i360_launcher_beta_01_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name="com.gmodelo.cutoverback.Activity.UpDownActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.gmodelo.cutoverback.Activity.RouteActivity"
            android:exported="false" />
        <activity
            android:name="com.gmodelo.cutoverback.Activity.MatchCodeMaterialActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.gmodelo.cutoverback.Activity.ListMaterialActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.gmodelo.cutoverback.Activity.ListCarrilesActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
        <activity
            android:name="com.gmodelo.cutoverback.Activity.LoginActivity"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gmodelo.cutoverback.Activity.BeginCountActivity"
            android:screenOrientation="portrait"
            android:exported="false" />
    </application>

</manifest>